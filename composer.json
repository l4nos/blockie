{
  "name": "lanos/blockie",
  "description": "Simplified block.io framework for building blockchain gateways.",
  "keywords": ["laravel", "block.io", "bitcoin", "payments", "bitcoin"],
  "type": "library",
  "require": {
    "illuminate/console": "^8.37|^9.0",
    "illuminate/contracts": "^8.37|^9.0",
    "illuminate/database": "^8.37|^9.0",
    "illuminate/http": "^8.37|^9.0",
    "illuminate/log": "^8.37|^9.0",
    "illuminate/notifications": "^8.37|^9.0",
    "illuminate/routing": "^8.37|^9.0",
    "illuminate/support": "^8.37|^9.0",
    "illuminate/view": "^8.37|^9.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Robert Lane",
      "email": "rob@updev.agency"
    }
  ],
  "autoload": {
    "psr-4": {
      "Lanos\\Blockie\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Lanos\\Blockie\\BlockchainGatewayServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
